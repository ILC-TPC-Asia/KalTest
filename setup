export KALTESTROOT=`pwd`
export IMAKEINCLUDE="-I${KALTESTROOT}"
export PATH=${KALTESTROOT}/bin:$PATH
export DYLD_LIBRARY_PATH=${KALTESTROOT}/lib:$DYLD_LIBRARY_PATH
export LD_LIBRARY_PATH=${KALTESTROOT}/lib:$LD_LIBRARY_PATH
export ROOT_LIBRARIES=$ROOTSYS/lib
export ROOT_INCLUDE_DIRS=$ROOTSYS/include

export SOSYMLINK=true
